{
  "name": "QR Code Generator",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Generate QR codes for the current tab.",
  "icons": {
    "16": "javatar16.png",
    "48": "javatar48.png",
    "128": "javatar128.png"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "javatar16.png",
    "default_title": "Generate QR Code",
    "default_popup": "popup.html"
  }
}

